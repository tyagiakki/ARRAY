package ARRAY;

public class simliar {
	public static void main(String[] args) {
		int arr[]= {2,1,2,1,3,2,1,3};
		int n=arr.length;
		int count=0;
		for(int i=0;i<n-1;i++) {
			for(int j=i+1;j<n;j++) {
				
				if(arr[i]==arr[j]) {
					count++;
				}
			}
		}
		System.out.println(count);
	}
}
//OUTPUT:-7

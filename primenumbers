package rescursion;

public class primenumberfromanarray {
	public static void main(String[] args) {
	int arr[]= {2,5,6,9,7,11};
	int n=arr.length;
	for(int i=0;i<n;i++) {
		boolean isPrime=true;
		for(int j=2;j<arr[i];j++) {
			if(arr[i]%j==0) {
				 isPrime=false;
				break;
			}		
		}
		if(isPrime) {
			System.out.println(arr[i]+ " are prime numbers");
		}
	}
	}
}
